<h1>Listing cancer_symptoms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cancer_symptoms.each do |cancer_symptom| %>
  <tr>
    <td><%= cancer_symptom.name %></td>
    <td><%= cancer_symptom.desc %></td>
    <td><%= link_to 'Show', cancer_symptom %></td>
    <td><% if user_signed_in? %><%= link_to 'Edit', edit_cancer_symptom_path(cancer_symptom) %><%end%></td>
    <td><% if user_signed_in? %><%= link_to 'Destroy', cancer_symptom, confirm: 'Are you sure?', method: :delete %><%end%></td>
  </tr>
<% end %>
</table>

<br />

<% if user_signed_in? %><%= link_to 'New Cancer symptom', new_cancer_symptom_path %><% end %>
